{
  "name": "bib-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-host": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "android": "npm run build && npx cap run android",
    "build-dev": "npm run build && npx cap copy && cd android && ./gradlew assembleDebug && cd .. && npm run copy-build",
    "copy-build": "cp android/app/build/outputs/apk/debug/app-debug.apk ../ && cd .. && git add ."
  },
